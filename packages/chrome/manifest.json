{
  "manifest_version": 3,
  "name": "wesflo Mock Proxy",
  "version": "0.0.1",
  "description": "Proxy f√ºr REST API requests die an einen lokalen node (express) server umgeleitet werden.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "declarativeContent",
    "declarativeNetRequest",
    "alarms"
  ],
  "action": {
    "default_popup": "./dist/index.html",
    "default_icon": {
      "16": "./static/icon/icon16.png",
      "32": "./static/icon/icon32.png",
      "48": "./static/icon/icon48.png",
      "128": "./static/icon/icon128.png"
    }
  },
  "background": {
    "service_worker": "./dist/sw/background.js"
  },
  "host_permissions": ["<all_urls>"]
}