{
  "name": "wf-mock-server",
  "version": "0.0.1",
  "description": "API to prerender react (SSG)",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch './src/*' -x tsx src/server.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electronicpartnerio/browser-mock.git"
  },
  "author": "Florian Wessner",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/electronicpartnerio/browser-mock/issues"
  },
  "homepage": "https://github.com/electronicpartnerio/browser-mock#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "coverage",
      "mock",
      "mock/util"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.2",
    "vite": "^6.0.6"
  }
}
