{
  "name": "@wesflo/mock-server",
  "version": "0.0.2",
  "description": "API to prerender react (SSG)",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "wf-serve": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run prebuild && tsc",
    "prebuild": "rm -rf dist && mkdir dist",
    "publish": "npm run build && npm publish --access=public",
    "build:local": "npm run build && npm pack",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electronicpartnerio/browser-mock.git"
  },
  "author": "Florian Wessner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electronicpartnerio/browser-mock/issues"
  },
  "homepage": "https://github.com/electronicpartnerio/browser-mock#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "coverage",
      "mock",
      "mock/util"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.3",
    "jest": "^29.7.0",
    "typescript": "^5.7.2"
  }
}
